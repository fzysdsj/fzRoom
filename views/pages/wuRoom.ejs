                        $(this).append(html);
                        setTimeout(setTime, 2200);
                    }
                    if (lengths >= 10) {
                        $(".grades-n").css("background-image", 'url("/images/q11/f' + yes + '.png")');
                        $(".music-icon").addClass("hide");

                        if (yes > 0 && yes <= 5) {
                            $(".text-main11").css("background-image", 'url("/images/q11/bg1.jpg")');
                            $(".study-text").html('<img src="/images/q11/text1.png">');

